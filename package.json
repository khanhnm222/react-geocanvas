{
  "name": "react-geocanvas",
  "version": "1.0.0",
  "description": "A lightweight React library for rendering GeoJSON data directly onto HTML5 Canvas. react-geocanvas is designed for fast, map-free visualization of geographic data, supporting multiple GeoJSON geometry types with customizable styles and high performance rendering.",
  "license": "MIT",
  "author": "Your Name",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/khanhnm222/react-geocanvas.git"
  },
  "bugs": {
    "url": "https://github.com/khanhnm222/react-geocanvas/issues"
  },
  "homepage": "https://github.com/khanhnm222/react-geocanvas#readme",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "echo \"No tests yet\" && exit 0"
  },
  "peerDependencies": {
    "react": ">=17",
    "react-dom": ">=17"
  },
  "devDependencies": {
    "@types/geojson": "^7946.0.16",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "eslint": "^9.36.0",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  }
}
